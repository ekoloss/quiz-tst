{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "app",
  "entryFile": "main",
  "projects": {
    "account": {
      "type": "library",
      "root": "libs/account",
      "entryFile": "index",
      "sourceRoot": "libs/account/src",
      "compilerOptions": {
        "tsConfigPath": "libs/account/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "orm": {
      "type": "library",
      "root": "libs/orm",
      "entryFile": "index",
      "sourceRoot": "libs/orm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/orm/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}